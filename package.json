    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "source-map-loader": "^4.0.2",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@popperjs/core": "^2.11.8",
    "accordion-js": "^3.3.4",
    "apexcharts": "^3.45.1",
    "cleave.js": "^1.6.0",
    "dayjs": "^1.11.10",
    "filepond": "^4.30.6",
    "filepond-plugin-image-preview": "^4.6.12",
    "flatpickr": "^4.6.13",
