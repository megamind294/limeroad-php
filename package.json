{
  "name": "lineone",
  "version": "1.6.1",
  "description": "Multipurpose Admin UI Kit based on Tailwind CSS",
  "main": "index.js",
  "scripts": {
    "start": "npm run prod && npm run hot",
    "dev": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "mix --production"
